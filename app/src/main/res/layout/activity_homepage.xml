<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Homepage">
    <TextView
        android:id="@+id/homepg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Homepage"
        android:fontFamily="@font/poppinsbold"
        android:textSize="24sp"
        android:textColor="#1E293B"
        android:layout_marginTop="135dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/welcome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Welcome "
        android:textColor="#1E293B"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@+id/homepg"
        tools:layout_editor_absoluteX="36dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/user3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Username"
        android:textColor="#525BFF"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/homepg"
        app:layout_constraintStart_toEndOf="@+id/welcome"
        app:layout_constraintBaseline_toBaselineOf="@+id/welcome"
        tools:layout_editor_absoluteX="106dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/your"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Your"
        android:textColor="#1E293B"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/welcome"
        tools:layout_editor_absoluteX="36dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/email3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
      android:layout_marginStart="3dp"
        android:text="Email"
        android:textColor="#525BFF"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/user3"
        app:layout_constraintBaseline_toBaselineOf="@+id/your"
        app:layout_constraintStart_toEndOf="@+id/your"/>

    <TextView
        android:id="@+id/ketemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginStart="3dp"
        android:text="has been activated"
        android:textColor="#1E293B"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/user3"
        app:layout_constraintStart_toEndOf="@+id/email3"
        app:layout_constraintBaseline_toBaselineOf="@+id/email3"/>

    <TextView
        android:id="@+id/your2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Your"
        android:textColor="#1E293B"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/your"
        tools:layout_editor_absoluteX="36dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginStart="3dp"
        android:text="Phone"
        android:textColor="#525BFF"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/email3"
        app:layout_constraintStart_toEndOf="@+id/your"
        app:layout_constraintBaseline_toBaselineOf="@+id/your2"/>

    <TextView
        android:id="@+id/ketphone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginStart="5dp"
        android:text="has been registered"
        android:textColor="#1E293B"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/ketemail"
        app:layout_constraintStart_toEndOf="@+id/email3"
        app:layout_constraintBaseline_toBaselineOf="@+id/phone"/>

</androidx.constraintlayout.widget.ConstraintLayout>